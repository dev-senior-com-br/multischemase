{
  "name": "multischemase",
  "version": "1.0.1-beta.1",
  "description": "Multischemase é uma ferramenta de criação de banco de dados multi schemas e migração independente de estrutura para o Node.",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "del-cli --force ./dist",
    "build": "tsc",
    "migrate": "flyway -configFiles=conf/flyway.js migrate",
    "postbuild": "cpx '**/*.ts' ./dist && del-cli --force ./dist/node_modules && del-cli --force ./dist/examples",
    "format": "prettier --write \"lib/**/*.ts\" \"lib/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-senior-com-br/multischemase.git"
  },
  "keywords": [
    "multi",
    "schema",
    "agnostic",
    "migration"
  ],
  "author": "André Rezende",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dev-senior-com-br/multischemase/issues"
  },
  "homepage": "https://github.com/dev-senior-com-br/multischemase#readme",
  "dependencies": {
    "@john.redd/node-flywaydb": "^3.0.3",
    "aws-sdk": "2.368.0",
    "umzug-typescript-helper": "1.0.9"
  },
  "devDependencies": {
    "@deboxsoft/cpx": "^1.5.0",
    "@types/jest": "^25.2.3",
    "del-cli": "^3.0.1",
    "flywaydb-cli": "^0.8.3",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5",
    "prebuild": "^10.0.0",
    "prettier": "^1.19.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0"
  }
}
